{
    "name": "mauriziocingolani/yii2-site-template",
    "description": "Template Yii2 website.",
    "authors": [
        {
            "name": "Maurizio Cingolani",
            "email": "maurizio@mauriziocingolani.it"
        }
    ],
    "keywords": ["yii2", "website", "template"],
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.7",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-authclient": "*",
        "mauriziocingolani/yii2-fmwk-php": "1.0.*",
        "kartik-v/yii2-social": "*",
        "rmrevin/yii2-fontawesome": "*"
    },
    "require-dev": {
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
